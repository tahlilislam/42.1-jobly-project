{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Information officer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                20000
            ]
        },
        "equity": {
            "type": "string",
            "default": "",
            "title": "The equity Schema",
            "examples": [
                "0.001"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "watson-davis"
            ]
        }
    },
    "examples": [{
        "title": "Information officer",
        "salary": 20000,
        "equity": "0.001",
        "companyHandle": "watson-davis"
    }]
}